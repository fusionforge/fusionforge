{
	"provisioners": [
		{
			"type": "shell",
			"scripts": [
				"scripts/fusionforge.sh"
			],
			"override": {
				"virtualbox-ovf": {
					"execute_command": "echo 'vagrant'|sudo -S bash '{{.Path}}'"
				}
			}
		}
	],
	"builders": [
		{
			"type": "virtualbox-ovf",
			"source_path": "box.ovf",
			"ssh_username": "vagrant",
			"ssh_password": "vagrant",
			"ssh_wait_timeout": "30s",
		}
	]
}
