    </input>
    <output>
    <soap:body use="encoded" namespace="http://gforge.org" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
    </output>
    </operation>
    
    <operation name="bugList">
    <soap:operation soapAction="http://gforge.org/soap/SoapAPI.php" style="rpc"/>
    <input>
    <soap:body use="encoded" namespace="http://gforge.org" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
    </input>
    <output>
    <soap:body use="encoded" namespace="http://gforge.org" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
    </output>
    </operation>
    
    <operation name="bugAdd">
    <soap:operation soapAction="http://gforge.org/soap/SoapAPI.php" style="rpc"/>
    <input>
    <soap:body use="encoded" namespace="http://gforge.org" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
    </input>
    <output>
    <soap:body use="encoded" namespace="http://gforge.org" encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"/>
    </output>
    </operation>
    
<operation name="bugUpdate">
    <soap:operation soapAction="http://gforge.org/soap/SoapAPI.php" style="rpc"/>
    <input>
