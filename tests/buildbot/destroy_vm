#! /bin/sh 

scriptdir=$(dirname $0)
configdir=$scriptdir/config
. $(dirname $0)/common-vm

read_param_vm "$@"

# Start script for this engine must exist
if [ ! -x $scriptdir/${VMENGINE}/destroy ]
then 
	echo "Don't know how to destroy ${VMENGINE} engine"
	echo "$scriptdir/${VMENGINE}/destroy not found"
	exit 4
else
	$scriptdir/${VMENGINE}/destroy
fi
