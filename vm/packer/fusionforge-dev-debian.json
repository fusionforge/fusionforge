{
	"provisioners": [
		{
			"type": "file",
			"source": "file/Desktop",
			"destination": "/root/"
		},
		{
			"type": "file",
			"source": "file/bashrc-deb",
			"destination": "/root/.bashrc"
		},
		{
			"type": "shell",
			"scripts": [
				"scripts/base-debian.sh",
				"scripts/vagrant.sh",
				"scripts/virtualbox-debian.sh",
				"scripts/fusionforge-source.sh",
				"scripts/cleanup-debian.sh",
				"scripts/zerodisk.sh"
			]
		}
	],
	"builders": [
		{
			"type": "virtualbox-iso",
			"boot_command": [
				"<esc><wait>",
				"auto ",
				"url=http://{{ .HTTPIP }}:{{ .HTTPPort }}/preseed.cfg ",
				"netcfg/get_hostname=forge ",
				"netcfg/get_domain=internal ",
				"<enter>"
			],
			"disk_size": 7168,
			"guest_os_type": "Debian_64",
			"http_directory": "http",
			"iso_url": "http://cdimage.debian.org/cdimage/archive/7.5.0/amd64/iso-cd/debian-7.5.0-amd64-netinst.iso",
			"iso_checksum": "0a86e2fdef365f8bd626de7e754189291174b86a312c655fa6585eec3c6e5caa5e8f128e37a6989ae8d45743e158191d1b178d76bf338cb4512f0ee4aa9cda3c",
			"iso_checksum_type": "sha512",
			"ssh_username": "root",
			"ssh_password": "vagrant",
			"shutdown_command": "poweroff",
			"guest_additions_mode": "disable",
			"virtualbox_version_file": ".vbox_version",
			"vboxmanage": [
				[
					"modifyvm",
					"{{.Name}}",
					"--memory",
					"512"
				],
				[
					"modifyvm",
					"{{.Name}}",
					"--cpus",
					"1"
				]
			],
			"vm_name": "fusionforge-dev-debian"
		},
		{
			"type": "qemu",
			"iso_url": "http://cdimage.debian.org/cdimage/archive/7.5.0/amd64/iso-cd/debian-7.5.0-amd64-netinst.iso",
			"iso_checksum": "0a86e2fdef365f8bd626de7e754189291174b86a312c655fa6585eec3c6e5caa5e8f128e37a6989ae8d45743e158191d1b178d76bf338cb4512f0ee4aa9cda3c",
			"iso_checksum_type": "sha512",
			"shutdown_command": "poweroff",
			"disk_size": 7168,
			"format": "qcow2",
			"headless": false,
			"accelerator": "kvm",
			"http_directory": "http",
			"ssh_username": "root",
			"ssh_password": "vagrant",
			"vm_name": "fusionforge-dev-debian",
			"boot_command": [
				"<esc><wait>",
				"auto ",
				"url=http://{{ .HTTPIP }}:{{ .HTTPPort }}/preseed.cfg ",
				"netcfg/get_hostname=forge ",
				"netcfg/get_domain=internal ",
				"debian-installer/exit/poweroff=true",
				"<enter>"
			]
		}
	],
	"post-processors": [
		{
			"type": "vagrant",
			"output": "fusionforge-dev-debian.box"
		}
	]
}
