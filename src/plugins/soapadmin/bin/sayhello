#! /usr/bin/perl -w

my $world ;

my $config_path = `forge_get_config config_path`;
chomp $config_path;

do "$config_path/plugins/soapadmin/world.conf"
    or die "Cannot read $config_path/plugins/soapadmin/world.conf" ;

print STDOUT "Hello $world on STDOUT!\n" ;
print STDERR "Hello $world on STDERR!\n" ;
